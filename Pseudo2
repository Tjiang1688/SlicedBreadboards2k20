import java.util.Queue;
import org.firstinspires.ftc.teamcode.game.robot.PathSeg;
//this is the end of the extra stuff I figured out

public class createPathFromJewelsToCryptoBox extends _____ {

    Queue<PathSeg> paths = new jewelsToCryptoBox();

    //to reset the motors for the Op mode I guess
    public void resetMotors()  {
        leftMotor.setMode(DcMotor.RunMode.STOP_AND_RESET_ENCODER);
        rightMotor.setMode(DcMotor.RunMode.STOP_AND_RESET_ENCODER);
        leftMotor.setMode(DcMotor.RunMode.RUN_USING_ENCODER);
        rightMotor.setMode(DcMotor.RunMode.RUN_USING_ENCODER);
        leftMotor.setDirection(leftDefaultDir);
        rightMotor.setDirection(rightDefaultDir);
    }

    //uses PathSeg to create a path maybe
    public void queuePath(PathSeg path) {
        paths.add(path);
    }

    //I'd assume this was to stop it?
    public void stop(){
        leftMotor.setPower(0);
        rightMotor.setPower(0);
        resetMotors();
    }
    //senses if done if it is stop if not keep going
    public boolean pathDone() {
        pathSeg path = paths.peek();
    }

}
